PROCEDURE swap(VAR a:INTEGER, VAR b: INTEGER)
VAR
    temp: INTEGER;
BEGIN
    temp:= a;
    a:=b;
    b:=temp;
END
 procedure insertionSort(A: list of sortable items)
   n = length(A)
   for i = 1 to n - 1 do
       j = i
       while j > 0 and A[j-1] > A[j] do
           swap(A[j], A[j-1])
           j = j - 1
       end while
   end for
 end procedure